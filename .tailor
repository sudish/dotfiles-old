[DEFAULT]
verbose = True
git-command = /opt/local/bin/git
projects = haskell-mode, bbdb
patch-name-format=

[haskell-mode]
source = cvs:haskell-mode
target = git:haskell-mode
start-revision = INITIAL
root-directory = /Users/sj/src/git/haskell/haskell-mode
state-file = tailor.state
subdir = .

[cvs:haskell-mode]
module = fptools/CONTRIB/haskell-modes/emacs
repository = :pserver:anoncvs@cvs.haskell.org:/cvs

[git:haskell-mode]

[bbdb]
source = cvs:bbdb
target = git:bbdb
start-revision = INITIAL
root-directory = /Users/sj/src/git/bbdb
state-file = tailor.state
subdir = .

[cvs:bbdb]
module = bbdb
repository = :pserver:anonymous@bbdb.cvs.sourceforge.net:/cvsroot/bbdb

[git:bbdb]

# Local Variables:
# mode: conf-unix
# End:
