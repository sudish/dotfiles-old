[alias]
	st = status
	ci = commit
	co = checkout
	up = remote update --prune
	de = describe --all
	l  = log --stat --decorate
	lc = log ORIG_HEAD.. --stat --no-merges
	lg = log --oneline --decorate
	dc = diff --color-words
	changes  = diff --name-status -r
	diffstat = diff --stat -r
[clean]
	requireForce = true
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
	ui = auto
[color "diff"]
	commit = blue
[core]
	fsyncobjectfiles = true
	preloadindex = true
[diff]
	renames = copy
[gc]
	pruneexpire = 30 days
[github]
	user = sudish
[gui]
	pruneduringfetch = true
[merge]
	stat = true
	log = true
	verbosity = 2
	conflictstyle = diff3
[pack]
	threads = 0
[push]
	default = current
[user]
	name = Sudish Joseph
	email = sudish@gmail.com

# Local Variables:
# mode: conf-unix
# End:
